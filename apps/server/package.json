{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "engines": {
    "node": "14.x"
  },
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "dotenv -e ./env/.env.local nest start --watch",
    "start:dev:mock": "dotenv -e ./env/.env.mocking nest start --watch",
    "start:debug": "dotenv -e ./env/.env.local nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint:check": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "graphql:codegen": "ts-node src/genGraphqlSchema.ts",
    "migrate:prisma:local": "dotenv -e ./env/.env.local -- prisma migrate dev --name",
    "prisma:studio:local": "dotenv -e ./env/.env.local -- prisma studio",
    "prisma:gen:local": "dotenv -e ./env/.env.local -- prisma generate",
    "prisma:sync": "prisma db push --preview-feature",
    "prisma:seed:local": "dotenv -e ./env/.env.local -- ts-node prisma/seed",
    "prisma:seed": "ts-node prisma/seed",
    "docker:up": "docker compose up -d",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "yarn lint:check"
    ]
  },
  "dependencies": {
    "@apollo/gateway": "^0.35.1",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/graphql": "^8.0.2",
    "@nestjs/platform-express": "^7.6.15",
    "@prisma/client": "^3.1.1",
    "@types/ioredis": "^4.27.4",
    "@types/uuid": "^8.3.1",
    "apollo-server-express": "^2.25.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "graphql": "^15.5.1",
    "graphql-tools": "^7.0.5",
    "ioredis": "^4.27.9",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "shallow-equal-object": "^1.1.1",
    "ts-morph": "^11.0.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "prisma": "^3.1.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  }
}
